{
  "name": "vue-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@casl/ability": "^3.0.0",
    "@casl/vue": "^0.5.1",
    "axios": "^0.18.0",
    "copy-to-clipboard": "^3.1.0",
    "downloadjs": "^1.4.7",
    "is-valid-domain": "0.0.7",
    "jquery": "^3.3.1",
    "js-string-escape": "^1.0.1",
    "lodash": "^4.17.11",
    "register-service-worker": "^1.5.2",
    "trumbowyg": "^2.14.0",
    "uid": "0.0.2",
    "vue": "^2.5.22",
    "vue-breadcrumbs": "^1.2.0",
    "vue-currency-filter": "^3.2.3",
    "vue-flatpickr-component": "^8.1.1",
    "vue-i18n": "^8.0.0",
    "vue-meta": "^1.5.8",
    "vue-moment": "^4.0.0",
    "vue-recaptcha": "^1.1.1",
    "vue-router": "^3.0.1",
    "vue-router-multiguard": "^1.0.3",
    "vue-upload-component": "^2.8.19",
    "vuelidate": "^0.7.4",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@storybook/vue": "^4.1.11",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-pwa": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.8.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "vuetify-loader": "^1.0.5",
    "vuex-persist": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup/jest.js"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/mock.js",
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less)$": "identity-obj-proxy",
      "api-client": "<rootDir>/src/api/server/index.js",
      "vue$": "<rootDir>/node_modules/vue/dist/vue.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/tests/setup"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
